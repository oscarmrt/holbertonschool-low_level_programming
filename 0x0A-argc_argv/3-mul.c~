#include<stdio.h>
/**
 *  main - Entry point
 *
 * Description: write a program that prints all arguments it receives
 * @argc: Is the parameter to be checked
 * @argv: Is the parameter to be checked
 * Return: Always 0 (Success)
 */
int main(int argc, char *argv[])
{
	int k = 0;

	while (k < argc)
	{
	printf("%s\n", argv[k]);
	k++;
	}
	return (0);
}
